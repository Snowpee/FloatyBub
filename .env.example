# API 密钥配置
# 用于保护 TTS API 接口的访问密钥
VITE_API_SECRET=your-secret-api-key-here

# Fish Audio API 密钥
FISH_AUDIO_API_KEY=your-fish-audio-api-key-here

# Supabase 配置
# Supabase 项目 URL
VITE_SUPABASE_URL=https://your-project.supabase.co
# Supabase 匿名密钥（用于前端）
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# 功能开关
# 启用用户系统
VITE_ENABLE_USER_SYSTEM=true

# Vercel 部署时需要配置的环境变量
# API_SECRET=your-secret-api-key-here
# FISH_AUDIO_API_KEY=your-fish-audio-api-key-here
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
# VITE_ENABLE_USER_SYSTEM=true